Object.prototype.build_attribs = function( py,attr ){
  var bench = py.BENCH
  var flex = py.FLEX
  this.forEach(
    function(action,_){
      if(!bench && py.POS.toLowerCase().match(action)){
        // NOP //
      }
      else
      if(flex && py.POS.match(/WR|RB|TE/i)){
        // NOP //
      }
      else{
        attr.push( g_fantasy_attrs[ action ]( py ).addPostHTMLtab(2) )
      }
    }
  )
}
