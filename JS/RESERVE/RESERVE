function autodraftRemainingRound(){
  if(getCurrentRound()%2!=0){
    g_snakeDraft.Up()
    openNextRound()
    g_snakeDraft.Down()
    updateDraftBoard()
  }
  else{
    openNextRound()
    updateDraftBoard()
  }/*
  else{
    openNextRound()
    g_snakeDraft.Down()
    openNextRound()
    g_snakeDraft.Up()
    updateDraftBoard()
  }*/
  if(draftComplete()){
    unlockClubhouse()
  }
}
