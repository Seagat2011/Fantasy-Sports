    return gm
  }
)
for(var wk=3;wk<8;wk++){
  games.push([])
  var locked = {}
  while(games[ wk ].length<16){
    if(
    locked[i] || 
    (i==j) ||
    cache[i+","+j]
    ){
      i = ++I%32
    }
    else
    if(locked[j]){
      j = ++J%32
    }
    else
    if(i<j){
      var a = [i,j];
      games[ wk ].push( a )
      locked[i] = 1
      locked[j] = 1
      i = ++I%32
      j = ++J%32
    }
  }
}
