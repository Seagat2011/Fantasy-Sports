  g_available_division_matchups.map(function(conference,i) {
    var wk = []
    conference.iterate(function(division){
          division.iterate(
            function(div){
              div.map(
                function(game,j){
                  if(!(j in wk)){
                    wk.push([])
                  }
                  intraDivisioner(wk[j],game)
                }
              )
            }
          )
          return division
        }
      )
    result.push(wk)
    return conference
  })
