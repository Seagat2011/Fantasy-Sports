];
var j=4;
for(var wk=3;wk<8;wk++){
  games.push([])
  var locked = {}
  for(var i=0;i<32;i++){
    var k = (i+j)%32
    var a = [i,k]
    if(locked[i] || locked[k]){
      for(var b in cache){
        if(
        cache.hasOwnProperty(b) &&
        !locked[cache[b][0]] &&
        !locked[cache[b][1]]
        ){
          games[ wk ].push( cache[b] )
          locked[cache[b][0]] = 1
          locked[cache[b][1]] = 1
          delete cache[b]
          break
        }
      }
      cache[ a.join(",") ] = a
    }
    else{
      games[ wk ].push( a )
      locked[a[0]] = 1
      locked[a[1]] = 1
    }
  }
  j++
}
srcTranslated.textContent = JSON.stringify(games).replace(/\]\]\,/g,"]],\n");
