function hideDraftBoard(){
  draftBOARD.style.display = "none"
}
function showGamedayBoard(){
  fantasyBOARDS.style.display = "inline"
}
function unlockClubhouse(){
  hideDraftBoard()
  showGamedayBoard()
}
