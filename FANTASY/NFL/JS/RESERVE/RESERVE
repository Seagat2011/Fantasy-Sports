var games = [[182,89],[165,98],[158,99],[157,120],[148,129],[147,130]].map(
  function(gm,i,me){
    if(
    (i%2)
    ){ 
      var tmp = me[i-1][1]
      me[i-1][1] = me[i][0]
      me[i][0] = tmp
    }
    return gm
  }
);
