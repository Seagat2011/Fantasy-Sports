  }  
}

function maxTEAMS(){
  var N = 1 
  for(var i in g_CURRENT.LEAGUE){
    if(g_CURRENT.LEAGUE.hasOwnProperty(i)){
      N *= g_CURRENT.LEAGUE[i]
    }
  }
  return N
}

