  var u = this;
  var i = 0;
  for(var x in u){
    if(u.hasOwnProperty(x)){
      cb(x,i++,this)
    }
  }
}

function nflDraft(){
  var result = g_league_schedule.map(
      function wk(div){
        var u = []
        div.map(
          function(v,i){
            if(i%2){
              u.push(v)
            }
          }
        )
        return u
      }
  )
  srcTranslated.value = JSON.stringify(result,2,2)
}

