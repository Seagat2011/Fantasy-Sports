//
function signPlayerToTeam(py,team){
  if(isMetaSlot(py) && !py.FLEX){  
    pushToMetaSlot(py,team)  
  }
  else
  if(!py.FLEX){
    pushToRosterSlot(py,team)  
  }
  else{
    pushToFlexSlot(py,team)  
  }
  updateDraftRoster(py,team)
  updateDraftChart(py)
}
