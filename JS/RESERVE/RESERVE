// parse D IAA-III leagues
function nflDraft(){
  var ico = 24
  var result = [];
  var conf
  var div
  srcCode.value.replace(/\t+/g," ").split(/\n{3,}/g).map( 
    function(u,i){  // conference
      u.split(/\n+/g).map( 
        function(v,j){
          if(j==0){
            v.split(/:/).map(
              function(w,k){
                if(k==0){
                  conf = w
                }
                else{
                  div = w
                }
              }
            )
          }
          else{
            var a = v.split(/\s+[\d-]/g)
            result.push( '{ _ico:"i('+(ico++)+')",_team:"'+a[0]+'",_conf:"'+conf+'",_div:"'+div+'" },' ) 
          }
        }
      ) 
    }
  );
  srcTranslated.value = result.join("\n");
}

