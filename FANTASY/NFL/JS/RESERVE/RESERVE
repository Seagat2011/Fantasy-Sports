Object.prototype._playerDetailsAtIndex = function(i){
  var ph = function(){  // place-holder
      this.POS = ""
      this.FULLNAME = ""
      this.POS = ""
      this.SHOW_TEAM = ""
      this.PRK = ""
      this.PTS = ""
      this.AVG = ""
      this.LAST = ""
  }
  var ret = [""] // [new ph()]
  ret.sz = 0
  if(i in this){
    ret[0] = this[i]
    ret.sz++
  }
  return ret
}
Object.prototype._playerMetaDetailsAtIndex = function(i){
  var ph = function(){  // place-holder
      this.POS = ""
      this.FULLNAME = ""
      this.POS = ""
      this.SHOW_TEAM = ""
      this.PRK = ""
      this.PTS = ""
      this.AVG = ""
      this.LAST = ""
  }
  var ret = ["","",""] // [new ph(),new ph(),new ph()]
  ret.sz = 0
  if(i in this){
    this[i].map(
      function(u,i){
        ret[i] = u
        ret.sz++
      }
    )
  }
  return ret
}
