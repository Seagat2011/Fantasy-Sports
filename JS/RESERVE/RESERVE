function updateDraftRoster(py,team){
  if(py.TEAM == "available"){
    //var POS = py.FLEX?"FLEX":py.POS;
    py.TEAM = team
    delete g_draftRoster[ py.POS ][ py.PRK-1 ]
  }
}
